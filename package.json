{
  "name": "@flippico/design-system",
  "version": "0.8.8",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "rm -rf dist",
    "build": "yarn build:cdn && yarn tsc -p ./tsconfig.json && vite build",
    "build:cdn": "vite build --config vite-bundle.config.ts",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "new": "plop --plopfile scaffolding/index.js",
    "generate-icons": "node scripts/make-icons.js",
    "postinstall": "yarn generate-icons"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.4",
    "@lit/react": "1.0.0",
    "@open-wc/testing": "4.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "bootstrap-icons": "1.11.2",
    "composed-offset-position": "^0.0.4",
    "handlebars": "4.7.8",
    "handlebars-helpers": "0.10.0",
    "lit": "3.0.2",
    "plop": "4.0.0",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-uglify": "^6.0.4",
    "sinon": "17.0.1"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./dark.css": "./dist/dark.css",
    "./light.css": "./dist/light.css",
    "./common.css": "./dist/common.css"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/blocks": "7.5.3",
    "@storybook/builder-vite": "7.5.3",
    "@storybook/web-components": "7.5.3",
    "@storybook/web-components-vite": "7.5.3",
    "command-line-args": "5.2.1",
    "download": "8.0.0",
    "front-matter": "4.0.2",
    "globby": "14.0.0",
    "path": "0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recursive-copy": "2.0.14",
    "storybook": "7.5.3",
    "terser": "^5.26.0",
    "typescript": "5.2.2",
    "vite": "5.0.0",
    "vite-plugin-static-copy": "^1.0.0"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  }
